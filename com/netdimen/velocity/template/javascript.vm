function isValidPositive(s, fieldname, msg)
	{
	var newString = '';
	s = s.trim();
	for (var i = 0; i < s.length; i++)
		{
		var c = s.charAt(i);
		if (c != ',') newString = newString + c;
		}
	s = newString;
	for (var i = 0; i < s.length; i++)
		{
		var c = s.charAt(i);
		if (! ((c == '+')|| (c == '-')|| (c == '.')|| ((c >= '0') && (c <= '9'))))
			{ alert(msg); return false; }
		}
	if (s < 0) { alert(msg); return false; }
    return true;
	}

