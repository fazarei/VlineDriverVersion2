        if ( ( (yy % 4 == 0) && (yy % 100 != 0) ) || (yy % 400 == 0) )
        	ai=29;
        else
        	ai = 28;
	}
	if (valInt(dd)) {
        if (dd >= 1 && dd <= ai)
        	return (true);
		else {
			alert('$stringhelper.localizedEscapedMsg("JAVASCRIPT_017_DAYVALUERANGE")'+ ai + '.');
		}
	} else {
        alert('$stringhelper.localizedEscapedMsg("JAVASCRIPT_018_NUMERICDATE")');
	}
	return(false);
}
   
