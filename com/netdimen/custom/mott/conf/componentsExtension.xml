	<transactions>
		<transaction>
			<tx-code>R245M</tx-code>
			<feature>BSReports</feature>
			<access-level>ReadAccess</access-level>
			<class>com.netdimen.custom.mott.newreports.generator.ReportR245M</class>
			<requires-authentication>true</requires-authentication>
		</transaction>
		<transaction>
			<tx-code>LAUNCHWORKINGMANAGER</tx-code>
			<tx-type>HOMEPAGE</tx-type>
