			<class>com.netdimen.custom.mott.tx.LaunchWorkingManager</class>
			<requires-authentication>true</requires-authentication>
			<container-transaction>true</container-transaction>
		</transaction>
		<transaction>
			<tx-code>REDIRECTWITHUSERID</tx-code>
			<tx-type>HOMEPAGE</tx-type>
			<feature>System</feature>
			<access-level>WriteAccess</access-level>
			<class>com.netdimen.custom.mott.tx.RedirectWithUserid</class>
			<requires-authentication>true</requires-authentication>
