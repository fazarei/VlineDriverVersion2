<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="gridstyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">550dp</item>
        <item name="android:scrollbarStyle">insideInset</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:numColumns">1</item>
    </style>

    <style name="gridtextview" parent="@android:style/TextAppearance">
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="gridtextviewlink" parent="@android:style/TextAppearance">
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">#1122CC</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="gridtable">
        <item name="android:background">#520764</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="gridtablerow2">
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:background">#FFFFFF</item>
    </style>
    <style name="gridtablerow">
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:layout_marginLeft">3dp</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="gridheader">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">100dp</item>
        <item name="android:layout_marginRight">100dp</item>
        <item name="android:background">@drawable/shape_gridhedear</item>
    </style>  
    <style name="gridheaderchecklisttask">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginLeft">100dp</item>
        <item name="android:layout_marginRight">100dp</item>
        <item name="android:background">@drawable/shape_gridhedeartask</item>
    </style>    
    <style name="gridfooter">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">100dp</item>
        <item name="android:layout_marginRight">100dp</item>
        <item name="android:background">@drawable/shape_gridfooter</item>
    </style>
    
    <style name="gridtxthedear">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">22sp</item>
    </style>
    
     <style name="gridtxthedear2">
        <item name="android:layout_height">wrap_content</item>
