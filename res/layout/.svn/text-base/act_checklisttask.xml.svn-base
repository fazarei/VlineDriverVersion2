
<HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:background="@drawable/shape_background">
    <TableLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" 
        >
      
        <TableLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

            <TableRow
                android:id="@+id/tableRow1"
                style="@style/tablerowodd"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:id="@+id/textView1"
                    style="@style/txtviewtablemedium"
                    android:text="@string/ireceive"
                    android:layout_span="2"/>

                <CheckBox
                    android:id="@+id/checkBox1"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:enabled="false" />

                <TextView
                    android:id="@+id/textView15"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/traineesig"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:layout_span="2"    />

                <TextView
                    android:id="@+id/textView10"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/date" />

                <EditText
                    android:id="@+id/editText1"
                    style="@style/edittextdate"
                    android:background="@drawable/shape_edittext"
                    android:layout_span="2"   
					android:focusable="false">
                    <requestFocus />
                </EditText>

                <ImageView
                    android:id="@+id/imageView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:paddingRight="60dp"
                    android:src="@drawable/calendar"
                    android:visibility="invisible" />

            </TableRow>
            
            <TableRow
                android:id="@+id/tableRow2"
                style="@style/tablerowodd"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="30dp" >

                <TextView
                    android:id="@+id/textView12"
                    style="@style/txtviewtablemedium"
                    android:text="@string/trainingcomplete"
                    />
                <TextView
                    android:id="@+id/textView13"
                    style="@style/txtviewtablesmall"
                    />

                <CheckBox
                    android:id="@+id/checkBox2"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:enabled="false" />

                <TextView
                    android:id="@+id/textView16"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/trainersig"
                    android:textAppearance="?android:attr/textAppearanceLarge" 
                    android:layout_span="2"   />

                <TextView
                    android:id="@+id/textView11"
                    style="@style/txtviewtablesmall"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/date"
                    />

                <EditText
                    android:id="@+id/editText2"
                    style="@style/edittextdate"
                    android:background="@drawable/shape_edittext" 
                     android:layout_span="2"  
                     android:focusable="false"/>

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:paddingRight="60dp"
                    android:src="@drawable/calendar"
                    android:visibility="invisible" />

            </TableRow>
            
            <TableRow
                android:id="@+id/tableRow4"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp" >

                <TextView
                    android:id="@+id/textView17"
                    style="@style/gridtxthedear"
                    android:paddingLeft="10dp"
                    android:layout_span="2" />

                <TextView
                    android:id="@+id/textView13"
                    style="@style/gridtxthedear"
                    android:layout_span="3"
                    android:text="@string/guidepractice" />

                <TextView
                    android:id="@+id/textView14"
                    style="@style/gridtxthedear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_span="4"
                    android:paddingLeft="4dp"
                    android:text="@string/unguidepractice" />
           
            </TableRow>
                  
        <TableRow
            android:id="@+id/tableRow3"
            style="@style/gridheader" 
            android:layout_marginRight="40dp" 
        	android:layout_marginLeft="20dp"
        	android:layout_marginTop="10dp" >

            <TextView
                android:id="@+id/textView2"
                style="@style/gridtxthedear"
                android:layout_width="300dp"
                android:layout_marginLeft="10dp"
                android:text="@string/instructed"
                android:width="300dp"/>

            <TextView
                android:id="@+id/textView3"
                style="@style/gridtxthedear"
                android:layout_width="300dp"
                android:layout_marginLeft="10dp"
                android:text="@string/task"
                android:width="500dp" />

            <TextView
                android:id="@+id/textView4"
                style="@style/gridtxthedear"
                android:layout_width="80dp"
                android:layout_marginLeft="10dp"
                android:paddingLeft="10dp"
                android:text="@string/na" />

            <TextView
                android:id="@+id/textView5"
                style="@style/gridtxthedear"
                android:layout_width="60dp"
                android:layout_marginLeft="20dp"
                android:text="@string/d" />

            <TextView
                android:id="@+id/textView6"
                style="@style/gridtxthedear"
                android:layout_width="130dp"
                android:layout_marginLeft="10dp"
                android:text="@string/e" />

            <TextView
                android:id="@+id/textView7"
                style="@style/gridtxthedear"
                android:layout_marginLeft="10dp"
                android:text="@string/na"
                android:width="90dp" />

            <TextView
                android:id="@+id/textView8"
                style="@style/gridtxthedear"
                android:layout_width="70dp"
                android:layout_marginLeft="10dp"
                android:text="@string/d" />

            <TextView
                android:id="@+id/textView9"
                style="@style/gridtxthedear"
                android:layout_width="100dp"
                android:layout_marginLeft="10dp"
                android:text="@string/e" />

        </TableRow>

        <GridView
            android:id="@+id/gridView1"
            style="@style/gridstyle"
            android:layout_height="300dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="30dp" >

        </GridView>
<TableLayout>
        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <Button
                android:id="@+id/button1"
                style="@style/button"
                android:layout_width="200dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/sel_button"
                android:text="@string/save"
                android:visibility="invisible" />

        </TableRow>
</TableLayout>
    </TableLayout>
   </TableLayout>
</HorizontalScrollView>
